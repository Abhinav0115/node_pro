{
    "builds": [
        {
            "source": "index.js",
            "use": "@vercel/node"
        },
        {
            "source": "build/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/add",
            "destination": "bulid/index.html"
        },
        {
            "source": "/add",
            "destination": "bulid/index.html"
        },
        {
            "source": "/products",
            "destination": "index.js"
        },
        {
            "source": "/products/(.*)",
            "destination": "index.js"
        },
        {
            "source": "/",
            "destination": "bulid/index.html"
        },
        {
            "source": "/(.+)",
            "destination": "build/$1"
        }
    ]
}
